local RAW = Weld(torso,ra,CF(-1.1, 0.41, 0.51)*Angles(rad(-90), rad(0), 0))
local LAW = Weld(rarm,la,CF(1.6,0,0)*Angles(rad(3), rad(0), rad(-45)))
