local neck = torso:WaitForChild'Neck'
local Debounce = false

local SFX = function(Parent,Type)
Type = tostring(Type:lower())
local Sound = Instance.new('Sound',Parent)
game.Debris:AddItem(Sound,10)
if Type == 'gunshot' then
Sound.SoundId = 'rbxassetid://265294743'
--print'gs'
elseif Type == 'reload' then
Sound.SoundId = 'rbxassetid://137563489'
elseif Type == 'shellin' then
Sound.SoundId = 'rbxassetid://163915632'
--print'si'
end
return Sound
end;

Reload = function()
end;

Fire = function(TrailBool)
	TrailBool = true
	for bullet = 0, 5 do
				spawn(function()
	if char.Humanoid.Health < 1 then return end
    local rayPart         = Instance.new("Part", workspace)
    rayPart.Name          = "RayPart"
    rayPart.BrickColor    = BrickColor.new'Deep orange'
    rayPart.Anchored      = true
    rayPart.CanCollide    = false
    rayPart.TopSurface    = Enum.SurfaceType.Smooth
    rayPart.BottomSurface = Enum.SurfaceType.Smooth
    rayPart.formFactor    = Enum.FormFactor.Custom
    rayPart.Size          = Vector3.new(0.2, 0.2, 4)
    rayPart.Material = 'Neon'
--print'bul created'
			
Instance.new("BlockMesh", rayPart).Scale = Vector3.new(0.2, 0.2, 0.5)
	
	
	
    local bulletposition = (Barrel.CFrame*CF(0,3,0)).p
    local bulletvelocity = (Vector3.new(math.random(-spread,spread), math.random(-spread,spread), math.random(-spread,spread)))+( (Barrel.CFrame*CF(0,500,0)).p - bulletposition).unit*1000
    local bulletlastposition = bulletposition
    rayPart.CFrame = CFrame.new( bulletposition, bulletposition+bulletvelocity )
--print'set cf'
        while true do
	        local dt = wait()
            bulletlastposition = bulletposition
            bulletvelocity = bulletvelocity + (Vector3.new(0, 0, 0)*dt)
	        bulletposition = bulletposition + (bulletvelocity*dt)
            local Tbl = { char, rayPart,Remington_Model,fRemington_Model}  
            local ray = Ray.new(bulletlastposition,  (bulletposition - bulletlastposition))
            local hit, hitposition = workspace:FindPartOnRayWithIgnoreList( ray, Tbl )
--print'made ray'
	          if TrailBool == true and rayPart.Parent ~= nil then
	        local dst = (bulletlastposition-bulletposition).magnitude
	       local Trl = Instance.new('Part',workspace)
	       table.insert(Tbl, Trl)
	local t = Trl
	t.FormFactor = 3
	t.Size = Vector3.new(.2,.2,dst)
	t.CFrame = CFrame.new(bulletlastposition,bulletposition) * CF(0,0,-dst/2)
	t.Material = 272
	t.CanCollide = false
	t.Color = rayPart.Color
	t.Transparency = .1
	--reclr(t)
	t.Anchored = true
	local msh = Instance.new('BlockMesh',t)
	msh.Scale = Vector3.new(.25,.25,1)
	spawn(function()
	wait()
	local nmb = 20
	for i = 0,nmb do
	t.Transparency = t.Transparency + (1/nmb)
	game:service'RunService'.Stepped:wait()
	end
	t:Destroy()
	end)   
	end
	--print'passed trail'
	if (torso.Position - rayPart.Position).magnitude > 200 then
	 	rayPart:Destroy()
	 break
	 end
	
	
            if hit then
            if hit.Parent:findFirstChild("Humanoid") ~= nil then
            if not hit.Parent:findFirstChild("Humanoid"):IsA'Humanoid' then return end
				
            if hit.Parent:findFirstChild("Humanoid").Health <= 0 then
            rayPart.CFrame = CFrame.new(bulletposition, bulletposition+bulletvelocity)
            rayPart:Destroy()
               
            return end
            end
				if hit.Parent:findFirstChild("Humanoid") ~= nil then
				if not hit.Parent:findFirstChild("Humanoid"):IsA'Humanoid' then return end
					ypcall(function()
						HMSound:Play()
						HitM.ImageTransparency = 0
						wait(.1/1.5)
						HitM.ImageTransparency = 1
					end)
					if not hit.Parent then return end
					hit.Parent.Humanoid.Health = hit.Parent.Humanoid.Health - math.random(40,65)
				end
				if hit.Name == 'Head' and hit.Parent:findFirstChild("Humanoid") ~= nil then
				if not hit.Parent:findFirstChild("Humanoid"):IsA'Humanoid' then return end
				spawn(function()
				wait(.1)
				if hit.Parent:findFirstChild("Humanoid").Health <= 0 then
				local s = Instance.new('Sound',hit)
				s.SoundId = 'rbxassetid://142445692'
				s:Play()
				s:Destroy()
				end
				end)
				end
				if hit.Parent:IsA("Hat") and hit.Parent.Parent:findFirstChild("Humanoid") ~= nil then
					if not hit.Parent.Parent:findFirstChild("Humanoid"):IsA'Humanoid' then return end
					hit:BreakJoints()
				end
                bulletposition = hitposition
                rayPart.CFrame = CFrame.new(bulletposition, bulletposition+bulletvelocity)
                rayPart:Destroy()
                break
            end
	        rayPart.CFrame = CFrame.new(bulletposition, bulletposition+bulletvelocity)
       
    end
				end)				
	end
	spawn(function()
		spawn(function()
			local knb = 3
		for i = 1,knb do
			CurCam.FieldOfView = CurCam.FieldOfView + 1
			game:service'RunService'.Stepped:wait()
		end
		for i = 1,knb do
			CurCam.FieldOfView = CurCam.FieldOfView - 1
			game:service'RunService'.Stepped:wait()
		end
		end)
		local a,b = math.random(13,21),math.random(-12,12)
for i = 1,4 do
CurCam.CoordinateFrame = CurCam.CoordinateFrame:lerp(CurCam.CoordinateFrame*Angles(rad(a),rad(b),0),.1)
game:service'RunService'.Stepped:wait()
end;end)
	wait(0.05)
	end

Pump = function()
for i = 1,5 do
CurrentRightArmAngle = clerp(CurrentRightArmAngle,CF(0,-.55,-.5)*Angles(rad(20),0,0),.85)
CurrentLeftArmAngle = clerp(CurrentLeftArmAngle,CF(0,-1.25,0),.95)
wait()
end
local GS = SFX(rarm,'reload')
GS.Volume = 1
GS.Pitch = 1.25
GS:Play()

for i = 1,5 do
CurrentLeftArmAngle = clerp(CurrentLeftArmAngle,CF(0,-.45,0),.95)
wait()
end
------------------
for index,obj in next, Remington_Model:children() do
if obj.Name == 'PumpPart' then
spawn(function()
if obj:FindFirstChild'mot' then
local mot = obj:FindFirstChild'mot'
--print'found mot'
mot.C0 = mot.C0 * CF(0,0,.5)
end
end)end
end
for index,obj in next, Remington_Model:children() do
if obj.Name == 'PumpPart' then
spawn(function()
if obj:FindFirstChild'mot' then
local mot = obj:FindFirstChild'mot'
--print'found mot'
mot.C0 = mot.C0 * CF(0,0,.5)
end
end)end
end
------------------
for index,obj in next, Remington_Model:children() do
if obj.Name == 'PumpPart' then
spawn(function()
if obj:FindFirstChild'mot' then
local mot = obj:FindFirstChild'mot'
--print'found mot'
for i = 1,5 do
wait()
mot.C0 = clerp(mot.C0,CF(0,.25,0)*Angles(rad(90),0,0),.5)
end end
end)end
end
for index,obj in next, fRemington_Model:children() do
if obj.Name == 'PumpPart' then
spawn(function()
if obj:FindFirstChild'mot' then
local mot = obj:FindFirstChild'mot'
--print'found mot'
for i = 1,5 do
wait()
mot.C0 = clerp(mot.C0,CF(0,.25,0)*Angles(rad(90),0,0),.5)
end
end
end)end
end
------------------
for i = 1,5 do
CurrentLeftArmAngle = clerp(CurrentLeftArmAngle,CF(0,-1.25,0),.95)
wait()
end
------------------
for index,obj in next, Remington_Model:children() do
if obj.Name == 'PumpPart' then
spawn(function()
if obj:FindFirstChild'mot' then
local mot = obj:FindFirstChild'mot'
--print'found mot'
for i = 1,5 do
wait()
mot.C0 = clerp(mot.C0,CF(0,0,0)*Angles(rad(90),0,0),.5)
end end
end)end
end
for index,obj in next, fRemington_Model:children() do
if obj.Name == 'PumpPart' then
spawn(function()
if obj:FindFirstChild'mot' then
local mot = obj:FindFirstChild'mot'
--print'found mot'\
for i = 1,5 do
wait()
mot.C0 = clerp(mot.C0,CF(0,0,0)*Angles(rad(90),0,0),.5)
end end
end)end
end
------------------
wait()
for i = 1,7 do
CurrentRightArmAngle = clerp(CurrentRightArmAngle,CF(0,0,0)*Angles(0,0,0),.85)
CurrentLeftArmAngle = clerp(CurrentLeftArmAngle,CF(0,0,0),.95)
wait()
end
end;

Recoil = function(Int)
for i = 1,Int do
RTilt = i
wait()
end
for i = 1,Int do
RTilt = Int-i
wait()
end
end;

CanFire = true
Reloading = false

local CurrentRightArmAngle2 = CF(0,0,0)
local sp = spread
local Aimin = false

AimIn = function()
sway = false
CurCam.FieldOfView = 45
game:service'RunService'.Stepped:wait()
CurCam.FieldOfView = 35
Aimin = true
for i = 1,5 do
CurrentRightArmAngle2 = clerp(CurrentRightArmAngle2,CF(-1.15,0,-.2)*Angles(0,0,0),.8)
game:service'RunService'.Stepped:wait()
end
spread = spread - (sp/2)
end

AimOut = function()
sway = true
CurCam.FieldOfView = 45
game:service'RunService'.Stepped:wait()
CurCam.FieldOfView = 60
Aimin = false
for i = 1,5 do
CurrentRightArmAngle2 = clerp(CurrentRightArmAngle2,CF(0,0,0)*Angles(0,0,0),.8)
game:service'RunService'.Stepped:wait()
end
spread = sp
end

CanAimAndShoot = true
Mouse.Button1Down:connect(function()
if Reloading == false and CanFire == true and CanAimAndShoot then
CanFire = false
local GS = SFX(rarm,'GunShot')
GS.Volume = 1
GS.Pitch = 1.25
GS:Play()
Fire(false)
spawn(function()
--Recoil(20)
end)
Pump()
CanFire = true
end
end)

Mouse.Button1Up:connect(function()
	MouseDown = false
end)

Mouse.Button2Down:connect(function()
	Mouse2Down = true;
	if CanAimAndShoot then
	AimIn()
	end
end)

Mouse.Button2Up:connect(function()
Mouse2Down = false;
if CanAimAndShoot then
AimOut()
end
end)

local runnin = false
Mouse.KeyDown:connect(function(k)
kb = k:byte()
k = k:lower()
if k == 'q' and CanAimAndShoot then
AimIn()
elseif kb == 48 then
runnin = true
end
end)

Mouse.KeyUp:connect(function(k)
kb = k:byte()
k = k:lower()
if k == 'q' and CanAimAndShoot then
AimOut()
elseif kb == 48 then
runnin = false
end
end)

MakeGui = function(Scale)
Gui = Instance.new('ScreenGui',Plr:WaitForChild'PlayerGui')
HitM = Instance.new('ImageLabel',Gui)
HitM.Size = UDim2.new(0,85*2,0,85*2)
HitM.Position = UDim2.new(.5,-85,.5,-115)
HitM.BackgroundTransparency = 1
HitM.ImageTransparency = 1
HitM.Image = 'rbxassetid://176067512'
Frame = Instance.new('Frame',Gui)
Ammo = Instance.new('TextLabel',Frame)
Stored = Instance.new('TextLabel',Frame)
-------------
Frame.Size = UDim2.new(0,60*Scale,0,25*Scale)
Frame.BackgroundTransparency = .75
Frame.BorderSizePixel = 0
Frame.BackgroundColor3 = Color3.new(255,255,255)
Frame.Position = UDim2.new(1,-10+(-60*Scale),1,-10+(-25*Scale))
-------------
Ammo.Size = UDim2.new(.487,3,.72,0)
Ammo.BackgroundTransparency = 1
Ammo.TextColor3 = Color3.new(-1,-1,-1)
Ammo.FontSize = 'Size28'
Ammo.Font = 'SourceSansBold'
Ammo.Text = ''
-------------
Stored.Size = UDim2.new(.4,3,.5,0)
Stored.Position = UDim2.new(.6,-4,.35,0)
Stored.BackgroundTransparency = 1
Stored.TextColor3 = Color3.new(-1,-1,-1)
Stored.FontSize = 'Size28'
Stored.Font = 'SourceSansBold'
Stored.Text = ''
-------------
Mode = Instance.new('TextLabel',Frame)
Mode.Size = UDim2.new(.4,3,.5,0)
Mode.Position = UDim2.new(.6,-4,0,0)
Mode.BackgroundTransparency = 1
Mode.TextColor3 = Color3.new(-1,-1,-1)
Mode.FontSize = 'Size24'
Mode.Font = 'SourceSansLight'
Mode.Text = '[ SEMI ]'
-------------
Slash = Instance.new('TextLabel',Frame)
Slash.Size = UDim2.new(.4,3,.5,0)
Slash.Position = UDim2.new(.3,0,.24,0)
Slash.BackgroundTransparency = 1
Slash.TextColor3 = Color3.new(-1,-1,-1)
Slash.FontSize = 'Size60'
Slash.Font = 'SourceSansBold'
Slash.Rotation = 5
Slash.Text = '/'
-------------
end;

HW.C0 = torso.CFrame:toObjectSpace(head.CFrame)

MakeGui(3)

local LKPart = torso
local Camera = workspace.CurrentCamera
local CurrentRightArmAngle3 = CF(0,0,0)

MoveArms = function(mouse)
    mouse=Mouse
    local dir = -math.asin(mouse.UnitRay.Direction.unit.y)
    HW.C1 = clerp(HW.C1,Angles(dir,0,0),.5)
    RAW.C0 = clerp(RAW.C0,CF(.35,-1,0) * Angles(rad(45), 0, 0)*(CurrentRightArmAngle)*(CurrentRightArmAngle2)*(CurrentRightArmAngle3),.5)

	--[[
	local Target = Camera.CoordinateFrame * CFrame.new(0, 0, -95)
	local headpos = (ts.CFrame * CFrame.new(0,2,0)).p
	RAW.C1 = lerp(RAW.C1,((CFrame.new(headpos, Target.p) - headpos):inverse()) * (ts.CFrame - ts.Position)*(CurrentRightArmAngle)*(CurrentRightArmAngle2),.3)
	local TorsoTarget = CFrame.new(Target.x, ts.CFrame.y, Target.z)
	ts.CFrame = CFrame.new(ts.CFrame.p, TorsoTarget.p)
	--]]
end

local State = 'Idle'
local DebounceState = false
game:service'RunService'.RenderStepped:connect(function()

LAW.C0 = clerp(LAW.C0,(CurrentLeftArmAngle),.5)

MoveArms()
Camera = workspace.CurrentCamera

if runnin == true then
CanAimAndShoot = false
if Aimin == true and CanAimAndShoot == false then
AimOut()
end
hum.WalkSpeed = 30 else 
CanAimAndShoot = true
hum.WalkSpeed = 20 end

if (torso.Velocity*Vector3.new(1, 0, 1)).magnitude < 15 and DebounceState == false then
	State = 'Idle'
	--print'Idle'
elseif (torso.Velocity*Vector3.new(1, 0, 1)).magnitude >= 15 and (torso.Velocity*Vector3.new(1, 0, 1)).magnitude < 26 and DebounceState == false then
	State = 'Walking'
	--print'Walking'
elseif (torso.Velocity*Vector3.new(1, 1, 1)).magnitude >= 26 and DebounceState == false then
	State = 'Running'
	--print'Running'
end		

if State == 'Idle' then
if sway then
CurrentRightArmAngle3 = clerp(CurrentRightArmAngle3,CF(0,sin(tick()*(4))/25,0),.5)
else
CurrentRightArmAngle3 = clerp(CurrentRightArmAngle3,CF(0,sin(tick())/35,0),.01)
end
elseif State == 'Walking' then
CurrentRightArmAngle3 = clerp(CurrentRightArmAngle3,CF(sin(tick()*8)/20,sin(tick()*(8*2))/20,0),.5)
elseif State == 'Running' then
CurrentRightArmAngle3 = clerp(CurrentRightArmAngle3,CF(sin(tick()*11)/20,-1+sin(tick()*(11*2))/20,.2)*Angles(rad(45),rad(20),rad(-10)),.5)
elseif State == 'Testing' then

end

end)

Plr.CameraMode = 'LockFirstPerson'
Plr.Chatted:connect(function(msg)
--[[if msg == 'r-|' then
Plr.CameraMode = 'Classic'
CamModel:Destroy()
Plr.Character:BreakJoints()
end--]]
end)

